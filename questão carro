#include <stdio.h>

int main(void) {
    double v;
    printf("Velocidade que costuma dirigir (km/h): ");
    scanf("%lf", &v);

    if (v > 75) {
        printf("Resposta do guarda: Muito rapido (> 75 km/h)!\n");
    } else {
        if (v > 65) {
            printf("Resposta do guarda: Rapido (> 65 km/h).\n");
        } else {
            if (v > 55) {
                printf("Resposta do guarda: Um pouco acima (> 55 km/h).\n");
            } else {
                if (v > 45) {
                    printf("Resposta do guarda: Dentro do esperado (> 45 km/h).\n");
                } else {
                    printf("Resposta do guarda: Lento (< 45 km/h).\n");
                }
            }
        }
    }
    return 0;
}
